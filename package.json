{
  "name": "med-windsurf",
  "version": "1.0.0",
  "description": "Med-Windsurf Application",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "start": "npm run start:backend",
    "start:backend": "cd backend && node server.js",
    "dev:backend": "cd backend && nodemon server.js",
    "start:frontend": "cd frontend && npm start",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && npm install --workspace=frontend && npm install --workspace=backend"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
